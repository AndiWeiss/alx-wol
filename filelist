# this file contains names of files
# which are created during build time
# to forward information betwene scripts

# used to transport the curent active module
# from FetchSources to HandleInitrd
module_file=__current_module__

# keeps the name of the file where the
# version information shall be patched
versionpatch_file=__versionpatch_file__
patchext=.notversionpatched

# relative path to original module
original_path=drivers/net/ethernet/atheros/alx

# containe the kernel version of the last installed build
kernelver_file=__current_installed_kernel__
